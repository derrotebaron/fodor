  <flag type="gcc-machine" name="neon-for-64bits" domain-type="boolean_no" />

  <flag type="gcc-machine" name="slow-flash-data" domain-type="boolean_no" />

  <flag type="gcc-machine" name="restrict-it" domain-type="boolean_no" />
